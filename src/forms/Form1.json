{
  "title": "Dynamic Form Example",
  "fileName": "form1",
  "fields": [
    {
      "name": "name",
      "label": "Name",
      "type": "text",
      "placeholder": "Enter your name",
      "required": true,
      "validation": {
        "minLength": 5,
        "maxLength": 10
      }
    },
    {
      "name": "email",
      "label": "Email",
      "type": "text",
      "format": "email",
      "placeholder": "Enter your email",
      "required": true
    },
    {
      "name": "age",
      "label": "Age",
      "type": "text",
      "format": "number",
      "placeholder": "Enter your age",
      "validation": {
        "min": 18,
        "max": 100
      }
    },
    {
      "name": "gender",
      "label": "Gender",
      "type": "radio",
      "required": true,

      "options": ["Male", "Female", "Other"]
    },
    {
      "name": "language",
      "label": "Language",
      "type": "checkbox",
      "required": true,

      "options": ["Hindi", "English", "Marathi", "Gujarathi", "Other"]
    },
    {
      "name": "education",
      "label": "Education",
      "type": "dropdown",
      "options": ["B.E.", "M.E.", "M. Tech"]
    },
    {
      "name": "message",
      "label": "Message",
      "type": "textarea",
      "required": true,

      "placeholder": "Enter your message"
    }
  ],
  "submitButtoName": "Submit Form"
}
